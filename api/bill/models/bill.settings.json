{
  "kind": "collectionType",
  "collectionName": "bills",
  "info": {
    "name": "Bill"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "semester": {
      "type": "integer"
    },
    "tahun": {
      "type": "integer"
    },
    "bulan": {
      "type": "enumeration",
      "enum": [
        "Januari",
        "Februari",
        "Maret",
        "April",
        "Mei",
        "Juni",
        "Juli",
        "Agustus",
        "September",
        "Oktober",
        "November",
        "Desember"
      ]
    },
    "nominal": {
      "type": "biginteger"
    },
    "nominal_dibayar": {
      "type": "biginteger",
      "default": "0"
    },
    "tanggal_pembayaran": {
      "type": "date"
    },
    "Status": {
      "type": "string",
      "default": "Belum Dibayar"
    },
    "student": {
      "via": "bills",
      "model": "student"
    }
  }
}
