{
  "kind": "collectionType",
  "collectionName": "medical_histories",
  "info": {
    "name": "MedicalHistory",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "penyakit": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "boolean",
      "default": false
    },
    "keterangan": {
      "type": "text"
    },
    "tanggal": {
      "type": "date"
    },
    "student": {
      "model": "student",
      "via": "medical_histories"
    },
    "jenis": {
      "type": "enumeration",
      "enum": [
        "Umum",
        "Parah",
        "Bawaan"
      ],
      "default": "Umum"
    }
  }
}
