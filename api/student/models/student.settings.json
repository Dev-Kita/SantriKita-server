{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "student",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "nama": {
      "type": "string",
      "required": true
    },
    "tahun_masuk": {
      "type": "integer",
      "required": true
    },
    "tahun_keluar": {
      "type": "integer"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "student"
    },
    "medical_histories": {
      "collection": "medical-history",
      "via": "student"
    },
    "bills": {
      "collection": "bill",
      "via": "student"
    },
    "achievements": {
      "collection": "achievement",
      "via": "student"
    },
    "violations": {
      "collection": "violation",
      "via": "student"
    },
    "tanggal_lahir": {
      "type": "date"
    },
    "nis": {
      "type": "biginteger",
      "required": true
    },
    "kelas": {
      "via": "students",
      "model": "classroom"
    },
    "kamar": {
      "type": "string"
    },
    "notifications": {
      "collection": "notification",
      "via": "student"
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "jenis_kelamin": {
      "type": "string"
    },
    "teacher_activities": {
      "via": "students",
      "collection": "teacher-activity"
    },
    "student_aktivities": {
      "via": "students",
      "collection": "student-aktivity"
    }
  }
}
