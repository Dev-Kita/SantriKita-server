{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "student",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "nama": {
      "type": "string",
      "required": true
    },
    "tahun_masuk": {
      "type": "integer",
      "required": true
    },
    "tahun_keluar": {
      "type": "integer"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "student"
    },
    "medical_histories": {
      "via": "student",
      "collection": "medical-history"
    },
    "bills": {
      "via": "student",
      "collection": "bill"
    },
    "achievements": {
      "collection": "achievement",
      "via": "student"
    },
    "violations": {
      "collection": "violation",
      "via": "student"
    },
    "tanggal_lahir": {
      "type": "date"
    },
    "nis": {
      "type": "biginteger",
      "required": true
    },
    "kelas": {
      "via": "students",
      "model": "classroom"
    }
  }
}
