{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "student"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "nama": {
      "type": "string",
      "required": true
    },
    "classroom": {
      "type": "string",
      "required": true
    },
    "tanggal_lahir": {
      "type": "string"
    },
    "tahun_masuk": {
      "type": "integer",
      "required": true
    },
    "tahun_keluar": {
      "type": "integer"
    },
    "lesson_histories": {
      "via": "students",
      "collection": "lesson-history"
    },
    "silabuses": {
      "via": "students",
      "collection": "silabus"
    },
    "user": {
      "via": "student",
      "plugin": "users-permissions",
      "model": "user"
    },
    "medical_histories": {
      "via": "student",
      "collection": "medical-history"
    },
    "bills": {
      "via": "student",
      "collection": "bill"
    },
    "achievements": {
      "via": "student",
      "collection": "achievement"
    },
    "violations": {
      "via": "student",
      "collection": "violation"
    }
  }
}
